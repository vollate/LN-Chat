# vim: set fenc=utf-8 ft=taskini:
# see: https://github.com/skywind3000/asynctasks.vim/wiki/Task-Config

[project-init]
command=./dev.sh clean
cwd=<root>/server
output=terminal

[project-build]
command=./dev.sh debug
cwd=<root>/server
errorformat=%. %#--> %f:%l:%c
output=terminal

[project-run]
command=./ln_server
cwd=<root>/server/build-debug/src
output=terminal

